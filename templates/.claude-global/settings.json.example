{
  "skipDangerousModePermissionPrompt": true,
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "markitdown": {
      "command": "markitdown-mcp",
      "args": []
    },
    "imagesorcery": {
      "command": "imagesorcery-mcp",
      "args": []
    },
    "github": {
      "command": "uvx",
      "args": [
        "mcp-github"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    }
  },
  "permissions": {
    "allow": [
      "Bash(echo:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(tail:*)",
      "Bash(head:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git pull:*)",
      "Bash(gh pr:*)",
      "Bash(gh auth:*)",
      "Bash(python3:*)",
      "Bash(poetry:*)",
      "mcp__github__*",
      "mcp__playwright__*",
      "mcp__fetch__*",
      "WebSearch"
    ],
    "deny": [],
    "ask": [
      "Bash(rm:*)",
      "Bash(sudo rm:*)",
      "Bash(sudo:*)",
      "Bash(chmod:*)",
      "Bash(chown:*)",
      "Bash(git push --force:*)",
      "Bash(git reset --hard:*)",
      "Bash(apt:*)",
      "Bash(apt-get:*)",
      "Bash(pip install:*)"
    ]
  }
}
